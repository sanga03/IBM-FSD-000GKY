traversal 
(.class or #id)
  $(.class).each((index,element)=>{

  		$(element).css("---","----");
  })

  $(.class).sibilings().each(()=>{})

  $(.class).parents().each(()=>{})// all
  $(.class).parent() // only immediate

  Next neigbour
  $(.class).next()
  $(.class).nextAll().each(()=>{})

  prev neigbour
  $(.class).prev()
  $(.class).prevAll().each(()=>{})

  not
    $(.class).not('#id').each(()=>{})

$(.class).first()
.last()

